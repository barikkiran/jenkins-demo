<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

  <class name="com.fhe.entity.Account" table="account">
  
    <id name="accountNo" column="account_no"></id>
    
    <property name="accountHolderName" column="account_holder_nm"/>
    <property name="branch" column="branch"/>
    <property name="mobileNo" column="mobile_no"/>
    <property name="email" column="email_addrs"/>
    <property name="balance" column="balance"/>
  
  </class>


</hibernate-mapping>